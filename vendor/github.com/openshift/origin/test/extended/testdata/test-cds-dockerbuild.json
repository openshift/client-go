{
  "kind":"BuildConfig",
  "apiVersion":"v1",
  "metadata":{
    "name":"docker-build"
  },
  "spec":{
    "completionDeadlineSeconds": 5,
    "triggers":[],
    "source":{
      "type":"Dockerfile",
      "dockerfile":"FROM centos:7\nRUN sleep 10m"
    },
    "strategy":{
      "type":"Docker",
      "dockerStrategy":{
        "from":{
          "kind":"DockerImage",
          "name":"centos/ruby-22-centos7"
        }
      }
    }
  }
}
