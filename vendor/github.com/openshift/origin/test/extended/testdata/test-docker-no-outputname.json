{
  "kind": "BuildConfig",
  "apiVersion": "v1",
  "metadata": {
    "name": "test-docker",
    "labels": {
      "name": "test-docker"
    }
  },
  "spec": {
    "triggers": [],
    "source": {
      "type": "Git",
      "git": {
        "uri": "https://github.com/openshift/ruby-hello-world"
      }
    },
    "strategy": {
      "type": "Docker",
      "dockerStrategy": {
        "env": [
          {
            "name": "BUILD_LOGLEVEL",
            "value": "5"
          }
        ],
        "from": {
          "kind": "DockerImage",
          "name": "centos/ruby-22-centos7"
        }
      }
    }
  }
}