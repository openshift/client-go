{
  "kind": "BuildConfig",
  "apiVersion": "v1",
  "metadata": {
    "name": "test",
    "labels": {
      "name": "test"
    }
  },
  "spec": {
    "triggers": [],
    "source": {
      "type": "Binary",
      "binary": {}
    },
    "strategy": {
      "type": "Docker",
      "dockerStrategy": {
        "forcePull": true,
        "from": {
          "kind": "DockerImage",
          "name": "centos/ruby-22-centos7"
        },
        "env": [
          {
            "name": "BUILD_LOGLEVEL",
            "value": "5"
          }
        ]
      }
    },
    "output": {
      "to": {
        "kind": "ImageStreamTag",
        "name": "test:latest"
      }
    }
  }
}
